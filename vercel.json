{
    "rewrites" : [
        {
            "source": "/api/(.*)",
            "destination": "routes/api"
        }
    ],
    "builds":[
        {
            "src":"./server.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"server.js"
        }
    ]
}